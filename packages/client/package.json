{
  "name": "client",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "ng serve --open",
    "build": "ng build",
    "start:electron": "electron .",
    "build:electron": "ng build && electron-packager . Scrabble --icon ./resources/icon.icns --asar --ignore=/node_modules --ignore=/e2e --ignore=/src --overwrite --out=./build",
    "deploy": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --config ../.prettierrc.yaml --ignore-path ../.prettierignore --write \"src/**/*\"",
    "test": "ng test",
    "coverage": "ng test --code-coverage --sourceMap=true",
    "lint": "ng lint --format stylish",
    "lint:fix": "npm run lint -- --fix",
    "docker:dev": "ng serve --host 0.0.0.0 --disable-host-check",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "docker:storybook": "npm run docs:json && start-storybook -p 6006 -h 0.0.0.0",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.1.5",
    "@angular/cdk": "^12.2.1",
    "@angular/common": "12.1.5",
    "@angular/compiler": "12.1.5",
    "@angular/core": "12.1.5",
    "@angular/forms": "12.1.5",
    "@angular/material": "^12.2.1",
    "@angular/platform-browser": "12.1.5",
    "@angular/platform-browser-dynamic": "12.1.5",
    "@angular/router": "12.1.5",
    "@types/node-emoji": "^1.8.2",
    "howler": "^2.2.3",
    "http-status-codes": "^2.2.0",
    "marked": "^4.0.12",
    "ngx-uploadcare-widget": "^3.0.2",
    "node-emoji": "^1.11.0",
    "party-js": "^2.1.2",
    "rxjs": "~6.6.0",
    "socket.io-client": "^4.4.0",
    "socket.io-mock": "^1.3.2",
    "store2": "^2.14.2",
    "tslib": "^2.2.0",
    "uploadcare-widget": "^3.21.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.3",
    "@angular-eslint/builder": "12.3.1",
    "@angular-eslint/eslint-plugin": "12.3.1",
    "@angular-eslint/eslint-plugin-template": "12.3.1",
    "@angular-eslint/schematics": "12.3.1",
    "@angular-eslint/template-parser": "12.3.1",
    "@angular/cli": "~12.1.3",
    "@angular/compiler-cli": "12.1.5",
    "@babel/core": "^7.20.12",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/angular": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/testing-library": "0.0.13",
    "@types/events": "^3.0.0",
    "@types/howler": "^2.2.7",
    "@types/jasmine": "~3.8.0",
    "@types/lodash": "^4.14.178",
    "@types/marked": "^4.0.2",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "acorn": "^8.0.0",
